{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"lib": [
			"ES2020"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": false,
		"typeRoots": [
			"node_modules/@types",
			"src/typeing"
		],
	},
	"exclude": [
		"**/node_modules/*"
	]
}
